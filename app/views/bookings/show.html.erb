<div class="container p-3">

<h1>This is your current booking</h1>
  <div class="card border p-3 m-2">
    <img src=" <%= %> " alt="">
    <p> <%= @booking.booking_status %> </p>
    <p> $ <%= @booking.price %> </p>
    <p> <%= @booking.start_date.strftime('%d/%m/%Y %H:%M') %> - <%= @booking.end_date.strftime('%d/%m/%Y %H:%M') %> </p>
  </div>
</div>
